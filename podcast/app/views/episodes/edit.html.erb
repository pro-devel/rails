<div id="podcast_show">
	<div id="show_banner">
		<div class="wrapper">
			<h1>The RocketShip Podcast Dashboard</h1>
		</div>
	</div>

	<div id="links">
		<div class="wrapper_skinny">
			<ul>
				<li><%= link_to 'Episodes', authenticated_root_path  %></li>
				<li><%= link_to 'New Episode', new_user_episode_path(@user) %></li>
				<li><%= link_to 'Account Setting', edit_user_registration_path  %></li>
				<li><%= link_to 'View Podcast', user_path(@user)  %></li>
			</ul>
		</div>
	</div>

	<div id="form">
		<div class="wrapper_skinny">
			<%= render 'form' %>
		</div>
	</div>
</div>